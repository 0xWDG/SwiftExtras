{"variants":[{"paths":["\/documentation\/swiftextras\/swift\/string\/sdbmhash"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"abstract":[{"type":"text","text":"Hashed string"}],"hierarchy":{"paths":[["doc:\/\/SwiftExtras\/documentation\/SwiftExtras","doc:\/\/SwiftExtras\/documentation\/SwiftExtras\/Swift","doc:\/\/SwiftExtras\/documentation\/SwiftExtras\/Swift\/String"]]},"metadata":{"symbolKind":"property","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"sdbmhash"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}],"roleHeading":"Instance Property","extendedModule":"Swift","role":"symbol","externalID":"s:SS11SwiftExtrasE8sdbmhashSivp","modules":[{"relatedModules":["Swift"],"name":"SwiftExtras"}],"title":"sdbmhash"},"sections":[],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/SwiftExtras\/documentation\/SwiftExtras\/Swift\/String\/sdbmhash"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"sdbmhash"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Si","kind":"typeIdentifier","text":"Int"},{"kind":"text","text":" { "},{"kind":"keyword","text":"get"},{"kind":"text","text":" }"}],"languages":["swift"],"platforms":["macOS"]}]},{"content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This algorithm was created for sdbm (a public-domain reimplementation of ndbm) database library."},{"type":"text","text":" "},{"type":"text","text":"it was found to do well in scrambling bits, causing better distribution of the keys and fewer splits."},{"type":"text","text":" "},{"type":"text","text":"it also happens to be a good general hashing function with good distribution."},{"type":"text","text":" "},{"type":"text","text":"the actual function is hash(i) = hash(i - 1) * 65599 + str[i];"},{"type":"text","text":" "},{"type":"text","text":"what is included below is the faster version used in gawk."},{"type":"text","text":" "},{"type":"text","text":"[there is even a faster, duff-device version] the magic prime constant 65599 (2^6 + 2^16 - 1)"},{"type":"text","text":" "},{"type":"text","text":"was picked out of thin air while experimenting with many different constants."},{"type":"text","text":" "},{"type":"text","text":"this is one of the algorithms used in berkeley db (see sleepycat) and elsewhere."}]}],"kind":"content"}],"kind":"symbol","references":{"doc://SwiftExtras/documentation/SwiftExtras":{"url":"\/documentation\/swiftextras","role":"collection","title":"SwiftExtras","abstract":[],"type":"topic","identifier":"doc:\/\/SwiftExtras\/documentation\/SwiftExtras","kind":"symbol"},"doc://SwiftExtras/documentation/SwiftExtras/Swift":{"role":"collection","type":"topic","kind":"symbol","url":"\/documentation\/swiftextras\/swift","title":"Swift","identifier":"doc:\/\/SwiftExtras\/documentation\/SwiftExtras\/Swift","abstract":[]},"doc://SwiftExtras/documentation/SwiftExtras/Swift/String/sdbmhash":{"title":"sdbmhash","abstract":[{"text":"Hashed string","type":"text"}],"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"sdbmhash"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Si","text":"Int"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/SwiftExtras\/documentation\/SwiftExtras\/Swift\/String\/sdbmhash","url":"\/documentation\/swiftextras\/swift\/string\/sdbmhash","type":"topic"},"doc://SwiftExtras/documentation/SwiftExtras/Swift/String":{"url":"\/documentation\/swiftextras\/swift\/string","role":"symbol","abstract":[],"kind":"symbol","fragments":[{"kind":"keyword","text":"extension"},{"kind":"text","text":" "},{"kind":"identifier","preciseIdentifier":"s:SS","text":"String"}],"type":"topic","title":"String","identifier"